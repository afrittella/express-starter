{
  "name": "express-starter-template",
  "version": "1.0.0",
  "description": "Simple express/cors/bodyparser/jest template with Typescript",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint '*/**/*.{js,ts}' --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "env-cmd -f .env_local nodemon",
    "test": "env-cmd -f .env_test jest --colors --runInBand",
    "test:coverage": "env-cmd -f .env_test jest --coverage --colors --runInBand"
  },
  "author": {
    "email": "info@andreafrittella.com",
    "name": "Andrea Frittella"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
