{
  "name": "express-starter-template",
  "version": "1.0.0",
  "description": "Simple express/cors/bodyparser/jest starter kit with Typescript",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint '*/**/*.{js,ts}' --fix",
    "build": "tsc",
    "watch": "tsc --watch",
    "run:dev": "nodemon --config nodemon_dev.json",
    "run:prod": "nodemon",
    "start:dev": "env-cmd -f .env_local run-p watch run:dev",
    "start:prod": "env-cmd -f .env_prod run-s build run:prod",
    "test": "env-cmd -f .env_test jest --colors --runInBand",
    "test:coverage": "env-cmd -f .env_test jest --coverage --colors --runInBand"
  },
  "author": {
    "email": "info@andreafrittella.com",
    "name": "Andrea Frittella"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.5",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  }
}
