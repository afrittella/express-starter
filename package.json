{
  "name": "express-starter-template",
  "version": "1.0.0",
  "description": "Simple express/cors/bodyparser/jest template with Typescript",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint '*/**/*.{js,ts}' --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "env-cmd -f .env_local nodemon",
    "test": "env-cmd -f .env_test jest --colors --runInBand",
    "test:coverage": "env-cmd -f .env_test jest --coverage --colors --runInBand"
  },
  "author": {
    "email": "info@andreafrittella.com",
    "name": "Andrea Frittella"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
